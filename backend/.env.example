# Copy this file to .env and fill in your actual API keys

# OpenAI Configuration (optional - for GPT models)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic Configuration (optional - for Claude models)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Cohere Configuration (optional - for Cohere models)
COHERE_API_KEY=your_cohere_api_key_here

# OpenRouter Configuration (primary AI service)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Gemini Configuration (fallback AI service)
GEMINI_API_KEY=your_gemini_api_key_here

# Hugging Face Configuration (optional - for local models)
HF_TOKEN=your_hugging_face_token_here

# Database Configuration
DATABASE_URL=sqlite:///./dev.db

# Application Configuration
DEBUG=False
LOG_LEVEL=INFO